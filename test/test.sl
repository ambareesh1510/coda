(def melody (G#4:3 F#4:1 G#4:3 C#4:1 D#4:1 F#4:1 C#4:1 A#3:1 G#3:4))
(def frequencies (render 0.4 (melody)))
(def sin-wave (freq t)
    (* 1000 (sin (* freq t 2 3.1415))))
(def sin-wave-2 (freq t)
    (* 1000 (sin (* 0.75 freq t 2 3.1415))))
(def sum-sin-waves (freq t)
    (+ (sin-wave freq t) (sin-wave-2 freq t)))
(def amplitudes (map (sum-sin-waves) (frequencies)))
(write "touhou.wav" (amplitudes))
